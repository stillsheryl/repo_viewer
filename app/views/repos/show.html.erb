<h1>Your Public Repos</h1>

<div class="wrapper">
  <p><%= button_to "Back to Dashboard", '/dashboard', method: :get, class: "button" %></p>

  <p><%= button_to "View Private Repos", "/user/repos", method: :get, class: "button" %></p>
</div>

<ul>
  <% @repos.each do |repo| %>
  <li><h3><%= link_to repo.name, repo.url  %></li>
  <% end %>
</ul>
